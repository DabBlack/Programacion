package examenes.Examen20211021;

public class Ejercicio_E {

	public static void main(String[] args) {
		
		int num, limite = 7;
		
		num = (int) Math.round(Math.random() * 100);
		
		for (int i = 0; i < 100; i++){

			if (num == limite)
				System.out.println("Este numero acaba en 7");
		}
		
			if (num == limite + 10){
				System.out.println("Este numero acaba en 7");
			}
		
	}

}
