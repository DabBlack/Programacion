package examenes.Examen20211021;

public class Ejercicio_A {

	public static void main(String[] args) {
		
		int num = 0;
		
		for (int i = 0; i < 10; i++){
			
			System.out.println( Math.round(Math.random() * 100));
			
			if (i % 2 == 1) {
				System.out.println("Media de los numeros impares: " + i / 2);
			}
			
		}
	
		
		
		
		
		
		
		
		
		
	}

}
