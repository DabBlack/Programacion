package examenes.Examen20211021;

public class Ejercicio_C {

	public static void main(String[] args) {
		
		int num, limiteSuperior30 = 30, limiteInferior20 = 20, limiteSuperior = 50, limiteInferior = 40;
		
		for (int i = 0; i < 10; i++){
			
		
		num = (int) (Math.round(Math.random() * (limiteSuperior - limiteInferior)) + limiteInferior);
		
		if (num % 2 == 0)
			System.out.println("Numero generado: " + num);
		
		}
		
	}

}
